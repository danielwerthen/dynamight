<Window x:Class="Dynamight.ImageProcessing.CameraCalibration.StructuredLightWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StructuredLightWindow" Height="300" Width="300" KeyDown="Window_KeyDown">
    <Grid Name="DisplayGrid" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Image Name="Display" Loaded="Display_Loaded" Stretch="None"/>
    </Grid>
</Window>
