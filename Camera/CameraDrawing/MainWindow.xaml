<Window x:Class="CameraDrawing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="960" Width="1280" Loaded="Window_Loaded" KeyUp="Window_KeyUp">
    <Grid>
        <Image Width="640" Height="480" Name="ImageLeft" HorizontalAlignment="Left" VerticalAlignment="Top"></Image>
        <Image Width="640" Height="480" Name="DrawLeft" HorizontalAlignment="Left" VerticalAlignment="Top"></Image>
        <Image Width="640" Height="480" Name="ImageRight" HorizontalAlignment="Right" VerticalAlignment="Top"></Image>
        <Image Width="640" Height="480" Name="DrawRight" HorizontalAlignment="Right" VerticalAlignment="Top"></Image>
        <Image Width="640" Height="480" Name="ImageLeftBot" HorizontalAlignment="Left" VerticalAlignment="Bottom"></Image>
        <Image Width="640" Height="480" Name="DrawLeftBot" HorizontalAlignment="Left" VerticalAlignment="Bottom"></Image>
        <Image Width="640" Height="480" Name="ImageRightBot" HorizontalAlignment="Right" VerticalAlignment="Bottom"></Image>
    </Grid>
</Window>
